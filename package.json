{
  "name": "mkout",
  "version": "1.0.33",
  "description": "Render an abstract syntax tree",
  "author": "muji",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkdoc/mkout.git"
  },
  "bugs": {
    "url": "https://github.com/mkdoc/mkout/issues"
  },
  "keywords": [
    "markdown",
    "commonmark",
    "ast",
    "transform",
    "render",
    "html",
    "xml",
    "yaml"
  ],
  "dependencies": {
    "manual": "~1.0.2",
    "mkast": "~1.2.2",
    "string-repeater": "~1.0.3",
    "through3": "~1.1.5"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "coveralls": "~2.11.8",
    "istanbul": "~0.4.2",
    "js-yaml": "~3.5.5",
    "mocha": "~2.4.5"
  },
  "scripts": {
    "lint": "jshint . && jscs .",
    "clean": "rm -rf coverage",
    "test": "NODE_ENV=test mocha ${SPEC:-test/spec}",
    "cover": "NODE_ENV=test istanbul cover _mocha -- ${SPEC:-test/spec}",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "config": {
    "man": {
      "example": true
    }
  }
}
